<% provide(:title, "Email") %>
<div class='slide' id='slide1' data-slide='1' data-stellar-background-ratio='0.5'>
  <div class="container">
    <div class="transparency">
			<h2>
				<%= @email.email %>
			</h2>
			<div class="center">
				<p>
					<%= link_to 'Unsubscribe' , unsubscribe_path(@email.unsubscribe_hash), method: :delete %>
				</p>
		</div>
    </div>
  </div>
</div>