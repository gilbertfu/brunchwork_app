<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <script type="text/javascript" src='http://codeorigin.jquery.com/jquery-2.0.3.min.js'></script>
    <!--<script type="text/javascript" src='assets/javascripts/jquery.stellar.min.js'></script>
    <script type="text/javascript" src='assets/javascripts/waypoints.js'></script>
    <script type="text/javascript" src='assets/javascripts/jquery.easing.1.3.js'></script>
    <script type="text/javascript" src='assets/javascripts/main.js'></script>-->
    <%= stylesheet_link_tag 'application', media: 'all',
                                           'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>
  </head>
  <body>

  <!--<div class="container-fluid">-->
    <%= render 'layouts/header' %> 
      <% flash.each do |key, value| %>
      <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>
      <%= yield %>
  <!--</div>-->
  <%= render 'layouts/footer' %>
  <!--<div class="container-fluid"> -->
  </body>
</html>